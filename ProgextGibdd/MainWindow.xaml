<Window x:Class="ProgextGibdd.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProgextGibdd"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="800" MinHeight="700" MinWidth="1300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid Background="#d1eefc">
            <TextBlock Text="ГИБДД" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="18"></TextBlock>
            <Image Source="Resources\gibdd_logo.png" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="10 5 0 5"></Image>
        </Grid>
        <Frame Name="frmMain" Grid.Row="1" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
